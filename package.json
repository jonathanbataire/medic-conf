    "canonical-json": "0.0.4",
    "csv-parse": "^4.4.3",
    "googleapis": "^40.0.0",
    "jshint": "^2.10.2",
    "json2csv": "^4.5.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "opn": "^6.0.0",
    "pouchdb-adapter-http": "^7.1.1",
    "pouchdb-core": "^7.1.1",
