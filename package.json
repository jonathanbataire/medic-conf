    "svgo": "^1.2.2",
    "uglify-js": "^3.5.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-exclude": "^2.0.1",
    "chai-shallow-deep-equal": "^1.4.6",
    "eslint": "^5.16.0",
    "express-pouchdb": "^4.1.0",
